{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "UrlsConfig": {
    "ServerUrl": "https://localhost:5002",
    "RemoteApiUrl": "https://localhost:5000"
  },
  "UploadOptions": {
    "UploadPath": "C:\\filestore",
    "ChunkSize": 25,
    "Expiration": 180
  },
  "HardwareCheckOptions": {
    "MemoryThreshold": 4,
    "StorageThreshold": 500
  },
  "AllowedHosts": "*"
}
